			<div id="oorderList">
			</div>
			<script src="http://libs.baidu.com/jquery/1.11.1/jquery.min.js"></script>

			<section class="wrapper">
				<div class="xt-center">
					<!-- right -->
					<div id="xt-center">

						<div class="xt-table">
						</div>
						<div class="xt-fenye">
							<div class="xt-fenye-left">当前第 1 / 1 页,每页10条，共 5条记录</div>
							<div class="xt-fenye-right">
