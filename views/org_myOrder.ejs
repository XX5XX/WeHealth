							</div>
						</div>

						<table cellpadding="0" cellspacing="0" border="0" bgcolor="#dcdcdc" width="100%">

							<!--<table id="mm" border="1" >-->
							<tbody id="orderList"></tbody>
						</table>

						<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>

						<script>
                            function orgOrderTable() {
                                $.ajax({
                                    type: 'post',
                                    url: '/per_myOrder',
                                    success: function (jsonData) {
                                        var str = '<table cellpadding="0" cellspacing="0" border="0" bgcolor="#dcdcdc" width="100%">';
                                        str += '<tr height="25px" class="xt-table">'
                                        str += '<th>' + '订单编号' + '</th>';
                                        str += '<th>' + '下单账号' + '</th>';
                                        str += '<th>' + '商品名称' + '</th>';
                                        str += '<th>' + '商品数量' + '</th>';
                                        str += '<th>' + '支付金额' + '</th>';
                                        str += '<th>' + '下单时间' + '</th>';
                                        str += '<th>' + '订单状态' + '</th>';
                                        str += '</tr>'
                                        for (var j in jsonData) {
                                            var obj = jsonData[j];
                                            str += '<tr height="25px" class="xt-table" align="center">'
                                            str += '<td>' + obj.orderId + '</td>';
                                            str += '<td>' + obj.userName + '</td>';
                                            str += '<td>' + obj.goodsName + '</td>';
                                            str += '<td>' + obj.goodsAmount + '</td>';
                                            str += '<td>' + obj.payHc + '</td>';
                                            str += '<td>' + obj.payTime + '</td>';
                                            str += '<td>' + obj.state + '</td>';
                                            str += '</tr>'
                                        }
                                        str += '</table>';
                                        $('#oorderList').html(str);
                                    },
                                    error: function () {
                                        alert('错误！')
                                    }

                                })
                            }
                            orgOrderTable();
						</script>
					</div>
				</div>
			</section>
		</section><!-- end main -->
		
