	</header><!-- end header -->

	<section class="main clearfix">

		<section class="top">	
			<div class="wrapper content_header clearfix">
				<div class="work_nav">
							
					<ul class="btn clearfix">
						<li><a href="org_uploadData.ejs" class="previous" data-title="数据上传"></a></li>
						<li><a href="org_index.ejs" class="grid" data-title="主页"></a></li>
						<li><a href="org_hcStore.ejs" class="next" data-title="HC店铺"></a></li>
					</ul>							
					
				</div><!-- end work_nav -->
				<h1 class="title">数据获取</h1>
			</div>		
		</section><!-- end top -->

		<section class="wrapper">
			<div class="content"> 
				<div class="widget-box">
		        <div class="widget-title"> <span class="icon"> <i class="icon-align-justify"></i> </span>
		          <h5>设置获取条件</h5>
		        </div>

		        <div class="widget-content nopadding">
		          <form action="#" method="get" class="form-horizontal">
		            <div class="control-group">
		              <label class="control-label">性别 :</label>
		              <div class="controls">
		                <input type="radio" name="gender" value="1">男
		                <input type="radio" name="gender" value="1">女
		              </div>
		            </div>
		            <div class="control-group">
		              <label class="control-label">年龄 :</label>
		              <div class="controls">
		                <input type="text" class="span11" placeholder="Upper limit" />
		                <input type="text" class="span11" placeholder="Lower limit" />
		              </div>
		            </div>
		            <div class="control-group">
		              <label class="control-label">心率 :</label>
		              <div class="controls">
		              	<input type="radio" name="radio" value="1">获取心率数据
		              </div>
		            </div>
		            <div class="control-group">
		              <label class="control-label">血压 :</label>
		              <div class="controls">
		                <input type="radio" name="radio" value="1">获取血压数据
		              </div>
		            </div>
		            <div class="control-group">
		              	<label class="control-label">睡眠 :</label>
		              	<div class="controls">
		                	<input type="radio" name="radio" value="1">获取睡眠数据
		            	</div>
		            </div>
		            <div class="control-group">
		              	<label class="control-label">其他 :</label>
		              	<div class="controls">
		                	<input type="radio" name="radio" value="1">获取机构详细数据
		              	</div>
		            </div>
		            <div class="control-group">
		              	<label class="control-label">总数目 :</label>
		              	<div class="controls">
		                	<input type="text" class="span11" placeholder="Set the amount of data" />
		            	</div>
		            </div>
		            <div class="form-actions">
		            	<button type="submit" class="btn btn-success">确认获取</button>
		            </div>
		          </form>
		        </div>
		    </div>

				
			</div><!-- end content -->
		</section>
	</section><!-- end main -->

